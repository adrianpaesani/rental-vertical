<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for Customer -->
        <record id="seq_contract_normal_order" model="ir.sequence">
            <field name="name">Normal Contract</field>
            <field name="code">contract.contract</field>
            <field name="prefix">NC</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_contract_rental_order" model="ir.sequence">
            <field name="name">Rental Contract</field>
            <field name="code">contract.contract</field>
            <field name="prefix">RC</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        <!-- Sequences for Supplier -->
        <record id="seq_sup_contract_normal_order" model="ir.sequence">
            <field name="name">Supplier Normal Contract</field>
            <field name="code">contract.contract</field>
            <field name="prefix">SNC</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_sup_contract_rental_order" model="ir.sequence">
            <field name="name">Supplier Rental Contract</field>
            <field name="code">contract.contract</field>
            <field name="prefix">SRC</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>


    </data>
</odoo>
