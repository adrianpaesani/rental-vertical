<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record model="ir.module.category" id="module_category_history_file">
            <field name="name">History File</field>
        </record>
        <record id="group_read_history_file" model="res.groups">
            <field name="name">Read History File</field>
            <field name="category_id" ref="module_category_history_file" />
        </record>
        <record id="group_edit_history_file" model="res.groups">
            <field name="name">Edit History File</field>
            <field name="category_id" ref="module_category_history_file" />
            <field name="implied_ids" eval="[(4, ref('group_read_history_file'))]"/>
        </record>
    </data>
</odoo>
