<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="fsm_equipment_form_view_stock_swrent" model="ir.ui.view">
            <field name="name">fsm.equipment.form.swrent</field>
            <field name="model">fsm.equipment</field>
            <field name="inherit_id" ref="fieldservice_stock.fsm_equipment_form_view_stock"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='current_stock_location_id']" position="after">
                    <field name="analytic_account_id" />
                </xpath>
            </field>
        </record>
        
        <record id="fsm_equipment_tree_view_model_swrent" model="ir.ui.view">
            <field name="name">fsm.equipment.tree.swrent</field>
            <field name="model">fsm.equipment</field>
            <field name="inherit_id" ref="fieldservice_stock.fsm_equipment_tree_view_model"/>
            <field name="arch" type="xml">
                <field name="product_id" position="after">
                    <field name="analytic_account_id"/>
                </field>
            </field>
        </record>
        
    </data>
</odoo>